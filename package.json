    "argon2": "^0.40.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "kysely": "^0.27.2",
    "libphonenumber-js": "^1.10.55",
    "lodash": "^4.17.21",
    "maskdata": "^1.3.1",
    "nanoid": "^5.0.3",
    "nest-commander": "^3.12.2",
    "nestjs-cls": "^3.6.0",
    "nestjs-otel": "^5.1.5",
