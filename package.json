{
  "name": "temp",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "prestart:dev": "npm run migration:run",
    "prestart:debug": "npm run migration:run",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest/jest.unit.config.js --runInBand",
    "test:watch": "jest --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e:run": "npm run migration:run",
    "test:e2e:run": "jest --config jest/jest.e2e.config.js --runInBand",
    "test:e2e": "docker-compose -f docker-compose.e2e-test.yaml down -v && docker-compose -f docker-compose.e2e-test.yaml run -p 3000:3000 --rm api-service-e2e npm run test:e2e:run && docker-compose -f docker-compose.e2e-test.yaml down",
    "dev": "docker-compose down -v && docker-compose run -p 3000:3000 --rm api-service npm run start:debug && docker-compose down",
    "test:e2e:watch": "docker-compose -f docker-compose.e2e-test.yaml down -v && docker-compose -f docker-compose.e2e-test.yaml run --rm api-service-e2e npm run test:e2e:run -- --watchAll && docker-compose -f docker-compose.e2e-test.yaml down",
    "migration:generate": "ts-node -r tsconfig-paths/register src/core/database/kysley/migration-generate.ts --",
    "migration:run": "ts-node -r tsconfig-paths/register src/core/database/kysley/migration-runner.ts",
    "depcruise:archi": "depcruise --version && depcruise --config ./scripts/dc-graph.js --output-type archi src libs | dot -T svg | depcruise-wrap-stream-in-html > high-level-dependencies.html",
    "depcruise:test": "depcruise -v --config ./scripts/dc-forbidden-test.js -- src libs",
    "prepare": "husky install"
  },
