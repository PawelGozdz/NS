	"scripts": {
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"prestart:dev": "npm run migration:run",
		"prestart:debug": "npm run migration:run",
		"start:debug": "nest start --debug 0.0.0.0:9229 --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest --config jest/jest.unit.config.js --runInBand",
		"test:watch": "jest --watchAll",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"pretest:e2e:run": "npm run migration:run",
		"test:e2e:run": "jest --config jest/jest.e2e.config.js --runInBand",
		"test:e2e": "docker-compose -f docker-compose.test.yaml down -v && docker-compose -f docker-compose.test.yaml run --rm api-service-test npm run test:e2e:run && docker-compose -f docker-compose.test.yaml down",
		"test:e2e:watch": "docker-compose -f docker-compose.test.yaml down -v && docker-compose -f docker-compose.test.yaml run --rm api-service-test npm run test:e2e:run -- --watchAll && docker-compose -f docker-compose.test.yaml down",
		"migration:generate": "ts-node -r tsconfig-paths/register src/database/kysley/migration-generate.ts --",
		"migration:run": "ts-node -r tsconfig-paths/register src/database/kysley/migration-runner.ts"
	},
