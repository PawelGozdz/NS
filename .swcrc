{
	"$schema": "<https://json.schemastore.org/swcrc>",
	"sourceMaps": true,
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": true,
			"dynamicImport": true
		},
		"transform": {
			"legacyDecorator": true,
			"decoratorMetadata": true
		},
		"target": "es2020",
		"keepClassNames": true
	},
	"module": {
		"type": "commonjs",
		"noInterop": false
	}
}
